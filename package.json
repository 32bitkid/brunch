  "version":      "0.9.0-dev",
  "keywords":     ["backbone", "coffeescript", "jst", "jquery", "zepto", "framework", "brunch", "stylus", "eco", "underscore"],
  "homepage":     "http://brunch.io/",
  "author":       "Brunch team, twitter: @brunch",
  "repository": {
    "type": "git",
    "url": "https://github.com/brunch/brunch.git"
  },
  "bugs": {
